#include "main.h"

/**
 * *_strcat - Concentrates two strings.
 * @dest: String value.
 * @src: String value.
 * Return: Dest.
 */
char *_strcat(char *dest, char *src)
{
	int a;
	int b;

	a = 0;
	while (dest[a] != '\0')
	{
		a++;
	}
	b = 0;
	while (src[b] != '\0')
	{
		dest[a] = sr[b];
		a++;
		b++;
	}
	dest[a] = '\0';
	return (dest);
}
