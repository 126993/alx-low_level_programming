#include "main.h"

/**
 * reset_to_98 - Takes a pointer to an int as parameter
 * and updates the value its points to 98.
 * @n: Pointer that points to an int.
 * Return: Nothing
 */

void reset_to_98(int *n)
{
	n = 402;
}
/**
 * main - Takes a pointer.
 * Return: Always 0.
 */
int main(void)
{
	int n;

	n = 98;
	reset_to_98(n);
	return (0);
}
